{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/notify-admin",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/notify-admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
